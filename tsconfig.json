{
  "extends": "@tsconfig/node12/tsconfig.json",
  "include": [
    "packages/*/src/**/*.ts"
  ],
  "typedocOptions": {
    "disableSources": true,
    "entryDocument": "index.md",
    "entryPoints": [
      "./packages/selderee/src/selderee.ts",
      "./packages/plugin-htmlparser2/src/hp2-builder.ts"
    ],
    "excludePrivate": true,
    "githubPages": false,
    "hideBreadcrumbs": false,
    "includeVersion": false,
    "name": "API documentation",
    "readme": "none",
    "out": "docs"
  }
}
